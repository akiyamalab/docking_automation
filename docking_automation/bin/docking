#!/usr/bin/env python3
import os
import sys

# パッケージのルートディレクトリを取得
script_dir = os.path.dirname(os.path.abspath(__file__))
package_dir = os.path.dirname(script_dir)

# パッケージのディレクトリをPythonパスに追加
if package_dir not in sys.path:
    sys.path.insert(0, package_dir)

# CLIツールのメイン関数を実行
from docking_automation.presentation.cli.docking_cli import main

if __name__ == "__main__":
    sys.exit(main())